testData <- readRDS("//hades/share/nortogero/GeoMxTools_Testing/GitHub_Versions/GeomxTools-iss144-lessthanthree/GeomxTools-iss144-lessthanthree/inst/extdata/DSP_NGS_Example_Data/demoData.rds")

assayData=exprs(testData)
phenoData=phenoData(testData)
protocolData=protocolData(testData)
featureData=featureData(testData)
experimentData=experimentData(testData)
annotation = annotation(testData)
dimLabels = dimLabels(testData)

testExp <- NanoStringExperiment(
assayData=assayData,
phenoData=phenoData,
protocolData=protocolData,
featureData=featureData,
experimentData=experimentData,
annotation = annotation,
dimLabels = dimLabels
)

